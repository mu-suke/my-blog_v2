    "microcms-js-sdk": "3.1.2",
    "next": "13.5.7",
    "next-seo": "5.15.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
